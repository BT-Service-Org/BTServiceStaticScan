<?xml version="1.0"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>52.0</apiVersion>
	<isExposed>true</isExposed>
	<targets>
		<target>lightningCommunity__Page</target>
		<target>lightningCommunity__Default</target>
		<target>lightning__RecordPage</target>
	</targets>
<targetConfigs>
	<targetConfig targets="lightningCommunity__Default">
		<property label="Page Name" name="pageName" type="String" datasource="apex://GDC_MS_NavigationMenuItems"/>
		<property name="showImage" type="Boolean" label="Do you want to show the Banner Image?"/>
		<property name="gdcBannerText" type="String" label="Banner Text" default="GDC"/>
		<property name="gdcImageRelativePath" type="String" label="Image Relative Path - Static Resource" default="/banner1.jpeg"/>
		<property name="imageHeight" type="Integer" label="Banner Height" min="320" max="600" default="320"/>
	</targetConfig>
</targetConfigs>
</LightningComponentBundle>